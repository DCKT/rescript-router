{
  "name": "@dck/rescript-router",
  "jsx": {
    "version": 4
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "examples",
      "type": "dev"
    }
  ],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "namespace": false,
  "uncurried": true,
  "bs-dependencies": ["@rescript/react", "rescript-classnames"],
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header",
    "-open Belt",
    "-open Cx"
  ]
}
